block var

<!DOCTYPE html>
html(lang="en")
	head
		// *** META *** //
		include meta
		// *** VENDORS CSS *** //
		block library-css
		// *** CSS *** //
		include css
		block css
		// *** FAVICON *** //
		link(rel="shortcut icon", href="favicon.ico", type="image/x-icon")
		// *** TITLE *** //
		title #{pageTitle}
	body
		// *** NAV *** //
		block nav
			include nav
		// *** ASIDE *** //
		block aside
			include aside
		// *** MAIN *** //
		main
			block main
			// *** PAGE NAME *** //
			#page-verify(data-class=`${bodyClass}`)
		// *** OVERLAY *** //
		#overlay
		// *** FOOTER *** //
		block footer
			include footer
		// *** VENDORS JS *** //
		block library-js
		// *** JS *** //
		include js
		block js