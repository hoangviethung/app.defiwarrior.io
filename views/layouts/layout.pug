block var

<!DOCTYPE html>
html(lang="en")
	head
		// *** META *** //
		include meta
		// *** VENDORS CSS *** //
		link(rel="stylesheet", href="./vendors/fancybox/jquery.fancybox.css")
		block library-css
		// *** CSS *** //
		include css
		block css
		// *** FAVICON *** //
		link(rel="shortcut icon", href="favicon.ico", type="image/x-icon")
		// *** TITLE *** //
		title #{pageTitle}
	body
		.page
			// *** ASIDE *** //
			block aside
				include aside
			// *** ASIDE *** //
			.page__content
				// *** NAV *** //
				block nav
					include nav
				// *** MAIN *** //
				main
					block main
					// *** PAGE NAME *** //
					#page-verify(data-class=`${bodyClass}`)
					// *** POPUP DEFI WALLET CONNECT POPOP *** //
					include ../components/wallet_connect_popup
					// *** POPUP DEFI STAKE LP TOKENS POPOP *** //
					include ../components/stake_lp_tokens_popup
		// *** OVERLAY *** //
		#overlay
		// *** FOOTER *** //
		block footer
			include footer
		// *** VENDORS JS *** //
		script(src="./vendors/jquery/jquery.js")
		script(src="./vendors/fancybox/jquery.fancybox.js")
		script(src="./vendors/notify/notify.js")
		block library-js
		// *** JS *** //
		block js
		include js